{
  "AWSTemplateFormatVersion":"2010-09-09",
  "Description":"Launch nested CloudFormation stack for testing",
  "Resources":{
    "myStack":{
      "Type":"AWS::CloudFormation::Stack",
      "Properties":{
        "TemplateURL":"https://s3.amazonaws.com/cloudformation-templates-us-east-1/S3_Bucket.template",
        "TimeoutInMinutes":"60"
      }
    }
  },
  "Outputs":{
    "StackName":{
      "Value":{
        "Ref":"AWS::StackName"
      }
    },
    "OutputFromNestedStack":{
      "Value":{
        "Fn::GetAtt":[
          "myStack",
          "Outputs.BucketName"
        ]
      }
    }
  }
}